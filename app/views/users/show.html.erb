<div class="col-md-5"
<ul class="users">
  <% if @user.image_link==nil %>
  <li> <%=image_tag avatar_url(@user)%> </li>
  <% else %>
  <li> <%= image_tag @user.image_link %> </li>
  <%end%>
  <li><%= @user.name %></li>
  <li><%= @user.email %></li>
  <li><%= @user.created_at %></li>
</ul>
</div>
<div class='col-md-7'>

<%= render @posts %>

</div>
